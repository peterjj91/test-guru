<tr>
  <td><%= link_to test.title, admin_test_path(test) %></td>
  <td><%= test.level %></td>
  <td><%= test.questions.count %></td>
  <td><%= button_to t('helpers.submit.test.start'), start_test_path(test) %></td>
  <td>
    <%= link_to t('helpers.submit.test.edit'), edit_admin_test_path(test) %>
  </td>
  <td>
    <%= link_to t('helpers.submit.test.delete'), admin_test_path(test), method: :delete, data: {confirm: t('helpers.delete')} %>
  </td>
</tr>